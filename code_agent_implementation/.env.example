# OpenAI API Key (必需)
OPENAI_API_KEY=your_api_key_here

# OpenAI API 基础URL（可选，用于自定义API端点）
OPENAI_BASE_URL=https://api.openai.com/v1

# 模型设置
MODEL_NAME=gpt-4o
TEMPERATURE=0

# 代理设置
VERBOSE=True
DETAILED_PROMPT=False
SAFE_MODE=False
APPROVAL_NEEDED=True

# 安全设置
MAX_ITERATIONS=10
MAX_CONSECUTIVE_TOOL_CALLS=3
MAX_IDENTICAL_TOOL_CALLS=2
